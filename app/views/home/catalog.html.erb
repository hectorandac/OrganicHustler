<script>

    //MAIN LAYER
    var LAYER = null;

    // Get the modal
    var modal_quick = document.getElementById('quick_view');

    // Get the <span> element that closes the modal
    var span_quick = document.getElementById('quick_view_close');

    // When the user clicks on <span> (x), close the modal
    span_quick.onclick = function () {
        modal_quick.style.display = "none";
        $("body").removeClass("modal-open")
    };

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function (event) {
        if (event.target === modal_quick) {
            modal_quick.style.display = "none";
            $("body").removeClass("modal-open")
        }

        if (event.target === modal) {
            modal.style.display = "none";
        }
    };

    var stage_ov = null;

    function fitStageIntoParentContainer() {
        var container = document.querySelector('#wrapper-img-1');
        // now we need to fit stage into parent
        var containerWidth = container.offsetWidth;
        // to do this we need to scale the stage
        var scale = containerWidth / 1000;
        console.log(containerWidth);
        if (1000 * scale < 440) {
            stage_ov.width(1000 * scale);
            stage_ov.height(1000 * scale);
            stage_ov.scale({x: scale, y: scale});
            stage_ov.draw();
        } else {
            stage_ov.width(440);
            stage_ov.height(440);
            stage_ov.scale({x: (440 / 1000), y: (440 / 1000)});
            stage_ov.draw();
        }
    }

