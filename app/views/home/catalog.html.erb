            $("body").removeClass("modal-open");
            $('body').css('overflow', 'auto');
            $('body').css('position', 'relative');
        }

        if (event.target === modal) {
            modal.style.display = "none";
        }
    };

    function close_modal() {
        product.remove();
        modal_quick.style.display = "none";
